{
  "edit_predictions": {
    "mode": "subtle",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null
    },
    "enabled_in_text_threads": false
  },
  "agent": {
    "inline_assistant_model": {
      "provider": "zed.dev",
      "model": "claude-sonnet-4"
    },
    "play_sound_when_agent_done": true,
    "default_profile": "ask",
    "model_parameters": [],
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-sonnet-4-thinking"
    },
    "version": "2"
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "theme": "Catppuccin Mocha - No Italics",

  "languages": {
    "Typst": {
      "soft_wrap": "bounded",
      "show_wrap_guides": false
    }
  },

  "lsp": {
    "nil": {
      // or "nixd":
      "initialization_options": {
        "formatting": {
          "command": ["nixfmt"]
        }
      }
    }
  },

  "auto_install_extensions": {
    "nix": true,
    "html": true,
    "catppuccin": true,
    "toml": true,
    "dockerfile": true,
    "latex": true,
    "sql": true
  }
}
